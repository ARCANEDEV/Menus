{
    "name":        "arcanedev/menus",
    "description": "Menu builder for Laravel 5.",
    "homepage":    "https://github.com/ARCANEDEV/Menus",
    "keywords":    [
        "arcanedev", "laravel", "menu", "menus", "builder", "nav", "navbar", "navigation", "bootstrap"
    ],
    "authors":  [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/ARCANEDEV",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":               ">=5.5.9",
        "arcanedev/support": "~3.0"
    },
    "require-dev": {
        "phpunit/phpcov":    "~2.0|~3.0",
        "phpunit/phpunit":   "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\Menus\\": "src/"
        },
        "files": ["helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\Menus\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "testbench": "composer require --dev \"orchestra/testbench=~3.0\""
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
